{
  "name": "martinez-polygon-clipping",
  "version": "0.3.3",
  "description": "Martinez polygon clipping algorithm, does boolean operation on polygons (multipolygons, polygons with holes etc): intersection, union, difference, xor",
  "main": "dist/martinez.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "src/",
    "dist/"
  ],
  "scripts": {
    "test": "tap test/*.test.js -R spec",
    "bench": "node bench.js",
    "lint": "eslint ./src/",
    "test:watch": "nodemon --exec 'npm run test && npm run lint'",
    "start": "watchify ./demo/js/index.js -o ./demo/js/bundle.js -dv & http-server -p 3000",
    "start:win": "start watchify ./demo/js/index.js -o ./demo/js/bundle.js -dv && start serve",
    "build": "browserify -s martinez ./index.js -o ./dist/martinez.js && npm run min",
    "min": "uglifyjs ./dist/martinez.js -o ./dist/martinez.min.js -m --comments",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "polygon",
    "clipping",
    "boolean",
    "intersection",
    "union",
    "xor",
    "geometry",
    "martinez"
  ],
  "repository": {
    "url": "git+https://github.com/w8r/martinez.git",
    "type": "git"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "indent": [
        2,
        2
      ],
      "key-spacing": [
        "error",
        {
          "mode": "minimum"
        }
      ]
    }
  },
  "homepage": "https://github.com/w8r/martinez",
  "author": "Alex Milevski <info@w8r.name>",
  "license": "MIT",
  "devDependencies": {
    "@turf/union": "^4.6.0",
    "@types/geojson": "^1.0.2",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "benchmark": "^2.1.4",
    "browserify": "^13.0.1",
    "eslint": "^3.19.0",
    "eslint-config-mourner": "^2.0.1",
    "geojson-project": "^1.0.0",
    "http-server": "^0.10.0",
    "leaflet": "^1.2.0",
    "leaflet-editable": "^1.1.0",
    "load-json-file": "^2.0.0",
    "nodemon": "^1.9.2",
    "superagent": "^2.1.0",
    "tap": "^6.2.0",
    "tap-status": "^1.0.1",
    "typescript": "^2.2.2",
    "uglify-js": "^3.3.21",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "avl": "^1.4.1",
    "tinyqueue": "^1.2.0"
  }
}
